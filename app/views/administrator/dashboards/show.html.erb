<%= render(
  layout: "shared/administrator_layout",
  locals: { heading: t(".administrator_dashboard"), back_button: false }
) do %>
  <%= render(partial: "administrator/users/table", locals: { users: @users }) %>
  <%= link_to(
    t(".add_user"),
    new_administrator_user_path,
    class: "govuk-button",
    data: { module: "govuk-button" }
  ) %>
  <%= render(
    partial: "administrator/local_authorities/table",
    locals: { local_authority: @local_authority }
  ) %>
<% end %>
