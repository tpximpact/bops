<h2 class="govuk-heading-m">
  Check, tag and confirm documents
</h2>
<%= govuk_task_list(id_prefix: "confirm-documents-tasks", html_attributes: {id: "confirm-documents-tasks"}) do |task_list|
      render(partial: "workflow_task", locals: {
        id: "check-supplied-document",
        task: Validation::CheckSuppliedDocumentsTask.new(@planning_application),
        task_list:
      })

      render(partial: "workflow_task", locals: {
        id: "check-missing-documents",
        task: Validation::CheckMissingDocumentsTask.new(@planning_application),
        task_list:
      })

      render(partial: "workflow_task", locals: {
        id: "upload-redacted-documents",
        task: Validation::RedactDocumentsTask.new(@planning_application),
        task_list:
      })
    end %>
